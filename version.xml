<package name="tidetree" version="1.0.0">
  <depends on="BEAST.base" atleast="2.7.0" atmost="2.7.9"/>
  <depends on="BEAST.app" atleast="2.7.0" atmost="2.7.9"/>

  <service type="beast.base.core.BEASTInterface">
    <provider classname="tidetree.evolution.datatype.EditData"/>
    <provider classname="lineageTree.substitutionmodel.EditAndSilencingModel"/>
    <provider classname="lineageTree.tree.StartingTree"/>
    <provider classname="lineageTree.simulation.SimulatedAlignment"/>
    <provider classname="lineageTree.distributions.TreeLikelihoodWithEditWindow"/>
  </service>
</package>
